!!!
%html
  %head

    %meta{:charset => "UTF-8"}/
    %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}/
    %meta{:content => "", :name => "description"}/
    %meta{:content => "", :name => "copyright"}/
    %link{:href => "css/kickstart.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "css/overwrite.css", :media => "all", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js", :type => "text/javascript"}
    %script{:src => "js/kickstart.js", :type => "text/javascript"}
    %script{:src => "/application.js"}

  %body
    .grid
      .col_12
        %h1 Github Demo

        %section.examples
          .examples
            %h4 Premade examples with people
            %button.actorsWithSameStar.blue.medium Star on the same repo
            %span.error
            %span.success.time

            .results
              %button.cypherResults.orange Show/Hide Results
              %p.result


        %section.launch-cypher
          .custom-query
            %h4 Custom query
            %textarea.cypherQuery{:placeholder => "Cypher query", :cols => 30}
            %button.launchCypher.blue.medium Launch!
            %span.error
            %span.success.time

          .results
            %button.cypherResults.orange Show/Hide Results
            %p.result
